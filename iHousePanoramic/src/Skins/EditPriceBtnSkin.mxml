<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="16" height="16">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect x="0" y="0" width="16" height="16">
		<s:fill>
			<s:SolidColor alpha="0.01" color="#000000"/>
		</s:fill>
	</s:Rect>
	<s:Image x="0" y="0" alpha="0.5" source="@Embed('assets/edit2.png')"
			 alpha.disabled="0.1"
			 alpha.over="1"/>		
</s:Skin>
