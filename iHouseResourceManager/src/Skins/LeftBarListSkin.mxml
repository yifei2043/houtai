<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="100%" height="100%">
	<fx:Metadata>[HostComponent("spark.components.List")]</fx:Metadata>
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<s:DataGroup id="dataGroup" x="0" y="0" width="100%" height="100%" buttonMode="true" useHandCursor="false"
				 clipAndEnableScrolling="true">
		<s:layout>
			<s:TileLayout horizontalGap="15" verticalGap="15"/>
		</s:layout>
	</s:DataGroup>
	<s:VScrollBar right="0" y="0" height="100%" fixedThumbSize="true"
				  skinClass="Skins.VScrollSkin1" viewport="{dataGroup}"/>
</s:Skin>